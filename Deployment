apiVersion: apps/v1
kind: Deployment
metadata: 
  name: devops-apps
spec:
  replicas: 2
  selector:
    matchLabels:
      app: devops
  template:
    metadata:
      labels:
        app: devops
    spec:
      containers:
      - name: devops-containers
        image: devops-apps:latest
        ports:
        - containerPort: 3200
